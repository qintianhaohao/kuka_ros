<launch>
    <!-- 不使用仿真时间 -->
    <param name="/use_sim_time" value="false" />
    <arg name="sim" default="true" />

    <!-- gazebo -->
    <!-- <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="paused" value="false"/>
        <arg name="gui" value="true"/>
        <arg name="headless" value="false"/>
        <arg name="debug" value="false"/>
    </include> -->

    <!-- kr10 -->
    <group ns = "kr10">
        <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find kr10)/urdf/kr10.xacro' ns:=kr10" />
        <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
            <param name="tf_prefix" value="kr10" />
        </node>
        <include file="$(find kr10_moveit_config)/launch/move_group.launch" />
        <node name="arbotix" pkg="arbotix_python" type="arbotix_driver" output="screen">
            <rosparam file="$(find kr10)/config/arm.yaml" command="load" />
            <param name="sim" value="true"/>
        </node>
        <!-- <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf"  args="-urdf -model kr10 -x 0 -y 0 -z 0 -Y 0 -param robot_description" /> -->


    </group>

    <!-- kr60 -->
    <group ns = "kr60">
        <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find kr60)/urdf/kr60.xacro' ns:=kr60" />

        <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
            <rosparam param="source_list">[move_group/fake_controller_joint_states]</rosparam>
        </node>
        <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
            <param name="tf_prefix" value="kr60" />
        </node>
        <include file="$(find kr60_moveit_config)/launch/move_group.launch" >
            <arg name="fake_execution" value="true"/>
        </include>
        <!-- <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf"  args="-urdf -model kr60 -x 5 -y 0 -z 0 -Y 0 -param robot_description" /> -->
    </group>

    <!-- tf -->
    <node pkg="tf" type="static_transform_publisher" name="kr10_to_kr60" args="3 0 0 3.14 0 0 /kr10/dummy /kr60/dummy 100"/>




    <!-- move_group -->


    <!-- 启动rviz可视化界面 -->

</launch>
